#bios.fcsm
#the BIOS for the farcpu system

MOVNR 2$256 AL
MOVNR 2$280 BL
MOVNR 1$0   AB 
SJP @14
INC AL
MOVRRM AB AL
JNE

MOVNM 1$102 @100 #f
MOVNM 1$97 @101 #a
MOVNM 1$114 @102 #r
MOVNM 1$99 @103 #c
MOVNM 1$112 @104 #p
MOVNM 1$117 @105 #u

HWU $8

RET #for testing porposes only before actual implementation

JMP &0 #jump to main program
